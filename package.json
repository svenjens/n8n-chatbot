{
  "name": "chatguuspt",
  "version": "1.0.0",
  "description": "ChatGuusPT - Vriendelijke chatbot voor de Koepel zakelijke gebruikers",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run copy:examples && npm run build:functions",
    "build:functions": "echo 'Netlify Functions will be built automatically'",
    "copy:examples": "node scripts/copy-examples.js",
    "preview": "vite preview",
    "start": "node src/server/index.js",
    "dev:server": "nodemon src/server/index.js",
    "dev:netlify": "netlify dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "setup": "node scripts/setup.js",
    "setup:mongodb": "./scripts/setup-mongodb.sh",
    "test:mongodb": "node scripts/test-mongodb.js",
    "lint": "echo 'âœ… Linting passed - no linter configured'",
    "deploy": "netlify deploy --prod",
    "deploy:preview": "netlify deploy",
    "lighthouse": "npx lighthouse-ci autorun",
    "audit": "npm audit --audit-level high",
    "validate:deployment": "node scripts/validate-deployment.js",
    "prepare:deploy": "npm run validate:deployment && git add . && git status"
  },
  "keywords": [
    "chatbot",
    "n8n",
    "openai",
    "embeddable",
    "widget"
  ],
  "author": "Sven Jens",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.4.0",
    "google-spreadsheet": "^4.1.2",
    "mongodb": "^6.19.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "netlify-cli": "^17.0.0",
    "nodemon": "^3.0.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
